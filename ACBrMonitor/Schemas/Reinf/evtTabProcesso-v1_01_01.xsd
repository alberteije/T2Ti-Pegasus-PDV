                                          </xs:annotation>
                                          <xs:simpleType>
                                            <xs:restriction base="xs:integer">
                                              <xs:pattern value="\d{1,14}"/>
                                            </xs:restriction>                                            
                                          </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="indSusp" minOccurs="1" maxOccurs="1">
                                          <xs:annotation>
                                            <xs:documentation>Indicativo de Suspensão</xs:documentation>
                                           </xs:annotation>
                                           <xs:simpleType>
                                            <xs:restriction base="xs:integer">
                                              <xs:pattern value="[0-9][0-9]"/>
                                            </xs:restriction>
                                          </xs:simpleType>                                          
