                                      <xs:annotation>
                                        <xs:documentation>Informar o número do processo administrativo/judicial.</xs:documentation>
                                      </xs:annotation>
                                      <xs:restriction base="xs:string">
                                        <xs:minLength value="1"/>
                                        <xs:maxLength value="21"/>
