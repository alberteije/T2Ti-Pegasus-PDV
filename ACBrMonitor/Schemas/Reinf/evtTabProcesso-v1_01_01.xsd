                                            <xs:annotation>
                                              <xs:documentation>
                                               Indicador da autoria do processo
                                                1-Próprio Contribuinte
                                                2-Outra Entidade ou Empresa
                                              </xs:documentation>
                                            </xs:annotation>
                                            <xs:restriction base="xs:unsignedInt">
                                              <xs:pattern value="[1|2]"/>
                                            </xs:restriction>
                                          </xs:simpleType>
                                        </xs:element>
                                      </xs:sequence>
                                    </xs:complexType>
                                  </xs:element>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                          </xs:sequence>
                        </xs:complexType>
                      </xs:element>
                      <xs:element name="alteracao" minOccurs="0" maxOccurs="1">
