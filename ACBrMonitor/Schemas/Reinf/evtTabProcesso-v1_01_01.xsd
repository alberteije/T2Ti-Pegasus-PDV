                                      <xs:restriction base="xs:string">
                                        <xs:length value="7"/>
                                        <xs:pattern value="20([0-9][0-9])-(0[1-9]|1[0-2])"/>
                                      </xs:restriction>
                                    </xs:simpleType>
                                  </xs:element>                                  
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="ideProcesso" minOccurs="1" maxOccurs="1">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="tpProc" minOccurs="1" maxOccurs="1">
