                                            <xs:restriction base="xs:string">                                            
                                              <xs:length value="2"/>
                                            </xs:restriction>
                                          </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="codMunic" minOccurs="0" maxOccurs="1">
                                          <xs:simpleType>
                                          <xs:restriction base="xs:integer">
                                            <xs:pattern value="\d{7}"/>
                                          </xs:restriction>
