				<xs:complexType>
					<xs:sequence>
						<xs:element name="qrCodNF3e">
							<xs:annotation>
								<xs:documentation>Texto com o QR-Code para consulta da NF-3e</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:whiteSpace value="preserve"/>
									<xs:minLength value="50"/>
									<xs:maxLength value="1000"/>
									<xs:pattern value="((HTTPS?|https?)://.*\?chNF3e=[0-9]{44}&amp;tpAmb=[1-2](&amp;sign=[!-ÿ]{1}[ -ÿ]{0,}[!-ÿ]{1}|[!-ÿ]{1})?)"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="ds:Signature"/>
		</xs:sequence>
	</xs:complexType>
