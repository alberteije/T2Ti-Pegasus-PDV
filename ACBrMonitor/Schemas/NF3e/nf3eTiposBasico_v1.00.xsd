								<xs:sequence>
									<xs:annotation>
										<xs:documentation>Sequencia XML</xs:documentation>
									</xs:annotation>
									<xs:element name="infAdFisco" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Informações adicionais de interesse do Fisco</xs:documentation>
